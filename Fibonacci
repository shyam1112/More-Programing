import java.util.*;

class Fibonacci{
    public static void main(String[] args) {
        
        Scanner sc=new Scanner(System.in);
        //input a number
        int n=sc.nextInt();
        int a=0;
        int b=1;
        int c=0;;
        //0,1,1,2,3,5,8,....
        for(int i=2;i<=n;i++){
            c=a+b;
            a=b;
            b=c;
        }
        System.out.println(c);
    }
}

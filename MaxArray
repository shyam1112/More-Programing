import java.util.*;
public class Maxvalue {
    public static void main(String[] args) {
        int[] arr={3,60,5,9,3,0};
        System.out.println(Max(arr));

    }
    static int Max(int[] arr){
        int max=arr[0];
        for(int i=1;i<arr.length;i++){
            if(max<arr[i]){
                max=arr[i];
            }
        }
        return max;
    }
}

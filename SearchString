import java.util.*;
public class SearchString {
    public static void main(String[] args) {
        String name="Shyam";
        char c='y';
        System.out.println(search(name,c));
    }

    static int search(String name,char c){
        if(name.length()==0){
            return -1;
        }
        for(int i=0;i<name.length();i++){
            if(name.charAt(i)==c){
                return i;
            }
        }
        return -1;
    }
}
